<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    app:layout_constrainedHeight="true"
    app:layout_constrainedWidth="true"
    tools:context=".pantallas.comunes.resultadoTransaccion.PantallaResultadoTransaccionFallida">


    <FrameLayout
        android:id="@+id/contenedorHeaderPantallaTransaccionFallida"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline176"
        app:layout_constraintBottom_toTopOf="@id/guideline177"
        app:layout_constraintStart_toEndOf="@id/guideline178"
        app:layout_constraintEnd_toStartOf="@id/guideline183"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0"
        />

    <TextView
        android:id="@+id/textView_PantallaTransaccionFallida"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline176"
        app:layout_constraintBottom_toTopOf="@id/guideline177"
        app:layout_constraintStart_toEndOf="@id/guideline178"
        app:layout_constraintEnd_toStartOf="@id/guideline183"
        app:layout_constraintVertical_bias="0.15"
        android:text="@string/textView_PantallaTransaccionFallida"
        android:fontFamily="@font/quicksand_regular"
        android:textSize="45dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="45dp"
        android:textAlignment="center"
        android:textColor="@color/azul"
        />

    <Button
        app:layout_constraintTop_toBottomOf="@id/textView_PantallaTransaccionFallida"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guideline177"
        app:layout_constraintStart_toEndOf="@id/guideline178"
        app:layout_constraintEnd_toStartOf="@id/guideline183"
        app:layout_constraintWidth_percent="0.4"
        app:layout_constraintHeight_percent="0.1"
        android:background="@drawable/boton_inicio"
        android:textColor="@color/blanco"
        android:textAllCaps="false"
        android:text="@string/button_PantallaTransaccionFallida"
        android:fontFamily="@font/quicksand_bold"
        android:textSize="20dp"
        android:onClick="salirTransaccionFallida"
        />





    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline176"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline177"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline178"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline183"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
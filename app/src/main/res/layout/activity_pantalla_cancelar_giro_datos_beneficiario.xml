<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/fondo"
    app:layout_constrainedWidth="true"
    app:layout_constrainedHeight="true"
    tools:context=".pantallas.corresponsal.usuarioComun.transacciones.giros.cancelar.pantallaCancelarGiroDatosBeneficiario">

    <FrameLayout
        android:id="@+id/contenedorHeaderCancelarGiroDatosBeneficiario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline9"
        app:layout_constraintStart_toEndOf="@id/guideline11"
        app:layout_constraintEnd_toStartOf="@id/guideline12"
        app:layout_constraintBottom_toTopOf="@id/guideline10"
        app:layout_constraintVertical_bias="0.0"/>


            <TextView
                app:layout_constraintBottom_toTopOf="@id/guideline10"
                app:layout_constraintStart_toEndOf="@id/guideline11"
                app:layout_constraintEnd_toStartOf="@id/guideline12"
                app:layout_constraintTop_toBottomOf="@id/contenedorHeaderCancelarGiroDatosBeneficiario"
                android:id="@+id/textView_CancelarGiroBeneficiario"
                android:layout_width="167dp"
                android:layout_height="0dp"
                android:layout_marginStart="36dp"
                android:fontFamily="@font/quicksand_regular"
                android:text="@string/textView_CancelarGiroDatosBeneficiario"
                android:textColor="@color/azul"
                android:textSize="21dp"
                app:layout_constraintVertical_bias="0.04"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintHeight_percent="0.09"/>

            <Spinner
                app:layout_constraintBottom_toTopOf="@id/guideline10"
                app:layout_constraintHeight_percent="0.07"
                app:layout_constraintVertical_bias="0.15"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintEnd_toStartOf="@id/guideline12"
                app:layout_constraintStart_toEndOf="@id/guideline11"
                app:layout_constraintTop_toBottomOf="@id/textView_CancelarGiroBeneficiario"
                android:id="@+id/spinner_tipoDocumentoBeneficiarioCancelarGiro"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="36dp"
                android:layout_marginEnd="45dp"
                android:background="@null"
                android:foreground="@drawable/spinner_style"
                android:overlapAnchor="false"
              />

            <EditText
                app:layout_constraintHeight_percent="0.07"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintVertical_bias="0.05"
                app:layout_constraintBottom_toTopOf="@id/guideline10"
                app:layout_constraintStart_toEndOf="@id/guideline11"
                app:layout_constraintEnd_toStartOf="@id/guideline12"
                app:layout_constraintTop_toBottomOf="@id/spinner_tipoDocumentoBeneficiarioCancelarGiro"
                android:id="@+id/editText_DocumentoBeneficiarioCancelarGiro"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="36dp"
                android:layout_marginEnd="45dp"
                android:background="@drawable/input_form"
                android:fontFamily="@font/quicksand_regular"
                android:hint="@string/editText_CancelarGiroNumeroDocumentoBeneficiario"
                android:imeOptions="actionDone"
                android:inputType="number"
                android:numeric="integer"
                android:paddingStart="23dp"
                android:textColor="@color/azul"
                android:textColorHint="@color/azul"
                android:textSize="21dp" />

            <Button
                app:layout_constraintVertical_bias="0.35"
                app:layout_constraintBottom_toTopOf="@id/guideline10"
                app:layout_constraintEnd_toStartOf="@id/guideline12"
                app:layout_constraintStart_toEndOf="@id/guideline11"
                app:layout_constraintTop_toBottomOf="@id/editText_DocumentoBeneficiarioCancelarGiro"
                android:id="@+id/button_ContinuarDatosBeneficiarioCancelarGiro"
                android:layout_width="138dp"
                android:layout_height="0dp"
                app:layout_constraintHeight_percent="0.095"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/boton_inicio"
                android:fontFamily="@font/quicksand_bold"
                android:onClick="continuarDatosBeneficiarioCancelarGiro"
                android:text="@string/button_CancelarGirosDatosBeneficiarioContinuar"
                android:textAllCaps="false"
                android:textColor="@color/blanco"
                android:textSize="20dp" />




    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
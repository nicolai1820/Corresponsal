<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".pantallas.corresponsal.administrador.inicio.pantallaInicialAdministrador">

    <FrameLayout
        app:layout_constraintBottom_toTopOf="@id/menuUser"
        app:layout_constraintEnd_toStartOf="@id/guideline79"
        app:layout_constraintStart_toEndOf="@id/guideline78"
        app:layout_constraintTop_toBottomOf="@id/guideline76"
        app:layout_constraintHeight_percent="0.90"
        android:id="@+id/contenedorFragmentUser"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_above="@+id/menuUser"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="5dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/menuUser"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/menu_fondo"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        app:itemIconSize="30dp"
        app:itemIconTint="@color/azul"
        app:itemTextColor="@color/azul"
        app:labelVisibilityMode="unlabeled"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/guideline78"
        app:layout_constraintEnd_toStartOf="@id/guideline79"
        app:layout_constraintHeight_percent="0.10"
        app:layout_constraintBottom_toTopOf="@id/guideline77"
        app:layout_constraintTop_toBottomOf="@id/contenedorFragmentUser"
        app:menu="@menu/menu_usuario_comun" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline78"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline79"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pantallas.comunes.header.LogoComercio">

    <!-- TODO: Update blank fragment layout -->
   <ImageView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:background="@drawable/borde_logo_comercio"
       android:src="@drawable/logo_comercio_borde"
       android:padding="4dp"/>

    <ImageView
        android:id="@+id/imageViewInactivo"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:background="@drawable/estado_inactivo_mpos"
        android:layout_marginStart="5dp"
        android:layout_marginTop="50dp"
        />

    <ImageView
        android:id="@+id/imageViewActivo"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:background="@drawable/estado_activo_mpos"
        android:layout_marginStart="5dp"
        android:layout_marginTop="50dp"
        android:visibility="invisible"/>

</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/fondo"
    app:layout_constrainedWidth="true"
    app:layout_constrainedHeight="true"
    tools:context=".pantallas.corresponsal.usuarioComun.transacciones.pagoFacturas.manual.manualCuentaRecaudadora.pantallaPagoFacturasManualCuentaRecaudadora">

    <FrameLayout
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/guideline88"
        android:id="@+id/contenedorHeaderPagoFacturasManualCuentaRecaudadora"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <TextView
        app:layout_constraintVertical_bias="0.04"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/contenedorHeaderPagoFacturasManualCuentaRecaudadora"
        android:id="@+id/textView_PantallaPagoFacturasManualCuentaRecaudadora"
        android:layout_width="283dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:fontFamily="@font/quicksand_regular"
        android:text="@string/textView_PagoFacturasManualCuentaRecaudadora"
        android:textColor="@color/azul"
        android:textSize="21dp" />

    <Spinner
        app:layout_constraintHeight_percent="0.07"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/textView_PantallaPagoFacturasManualCuentaRecaudadora"
        android:id="@+id/spinner_tipoDeCuentaCuentaRecaudadora"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="45dp"
        android:background="@null"
        android:foreground="@drawable/spinner_style"
        android:overlapAnchor="false"
        android:paddingStart="23dp" />

    <EditText
        app:layout_constraintHeight_percent="0.07"
        app:layout_constraintVertical_bias="0.05"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/spinner_tipoDeCuentaCuentaRecaudadora"
        android:id="@+id/editText_PagoFacturasManualNumeroCuentaRecaudadora"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/input_form"
        android:fontFamily="@font/quicksand_bold"
        android:hint="@string/editText_PagoFacturasManualNumeroCuentaRecaudadora"
        android:inputType="number"
        android:numeric="integer"
        android:paddingStart="23dp"
        android:textColor="@color/azul"
        android:textColorHint="@color/azul"
        android:textSize="21dp" />

    <EditText
        app:layout_constraintHeight_percent="0.07"
        app:layout_constraintVertical_bias="0.05"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/editText_PagoFacturasManualNumeroCuentaRecaudadora"
        android:id="@+id/editText_PagoFacturasManualCuentaRecaudadoraNumeroReferencia"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/input_form"
        android:fontFamily="@font/quicksand_bold"
        android:hint="@string/editText_PagoFacturasManualCuentaRecaudadoraNumeroReferencia"
        android:imeOptions="actionDone"
        android:inputType="number"
        android:numeric="integer"
        android:paddingStart="23dp"
        android:textColor="@color/azul"
        android:textColorHint="@color/azul"
        android:textSize="21dp" />

    <Button
        app:layout_constraintHeight_percent="0.095"
        app:layout_constraintBottom_toTopOf="@id/guideline89"
        app:layout_constraintVertical_bias="0.3"
        app:layout_constraintEnd_toStartOf="@id/guideline91"
        app:layout_constraintStart_toEndOf="@id/guideline90"
        app:layout_constraintTop_toBottomOf="@id/editText_PagoFacturasManualCuentaRecaudadoraNumeroReferencia"
        android:id="@+id/button_PagoFacturasManualCuentaRecaudadoraVamos"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="15dp"
        android:background="@drawable/boton_inicio"
        android:fontFamily="@font/quicksand_regular"
        android:onClick="pagoFacturasManualCuentaRecaudadoraVamos"
        android:text="@string/button_PagoFacturasManualCuentaRecaudadoraVamos"
        android:textAllCaps="false"
        android:textColor="@color/blanco"
        android:textSize="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline88"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline89"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>